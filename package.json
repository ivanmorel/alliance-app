{
  "private": true,
  "name": "alliance",
  "version": "2.5.3",
  "scripts": {
    "android-bundler": "adb reverse tcp:8081 tcp:8081",
    "android": "react-native run-android",
    "e2e:android:debug:build": "detox build -c android.debug",
    "e2e:android:debug:test": "detox test -c android.debug --workers 1 --jest-report-specs",
    "e2e:android:debug": "yarn e2e:android:debug:build; yarn e2e:android:debug:test",
    "e2e:android:release:build": "detox build -c android.release",
    "e2e:android:release:test": "detox test -c android.release -l verbose --headless --workers 1 --cleanup",
    "e2e:android:release": "yarn e2e:android:release:build; yarn e2e:android:release:test",
    "e2e:ios:debug:build": "detox build -c ios.debug",
    "e2e:ios:debug:test": "detox test -c ios.debug --workers 1 --cleanup --jest-report-specs",
    "e2e:ios:debug": "yarn e2e:ios:debug:build; yarn e2e:ios:debug:test",
    "e2e:ios:release:build": "detox build -c ios.release",
    "e2e:ios:release:test": "detox test -c ios.release -l verbose --workers 2 --cleanup",
    "e2e:ios:release": "yarn e2e:ios:release:build; yarn e2e:ios:release:test",
    "eas-build-post-install": "yarn add -D @bugsnag/source-maps react-native-dotenv metro-react-native-babel-preset",
    "eas-build-pre-install": "type sudo && sudo gem install cocoapods-bugsnag || true",
    "eas-build:all:development": "eas build -p all --profile development --non-interactive",
    "eas-build:all:feature": "eas build -p all --profile feature-test --non-interactive",
    "eas-build:all:gsd": "eas build -p all --profile gsd --non-interactive",
    "eas-build:all:preview": "eas build -p all --profile preview --non-interactive",
    "eas-build:all:production": "eas build -p all --profile production",
    "eas-build:android:development": "eas build -p android --profile development --non-interactive",
    "eas-build:android:feature": "eas build -p android --profile feature-test --non-interactive",
    "eas-build:android:gsd": "eas build -p android --profile gsd --non-interactive",
    "eas-build:android:preview": "eas build -p android --profile preview --non-interactive",
    "eas-build:android:production": "eas build -p android --profile production",
    "eas-build:ios:development": "eas build -p ios --profile development --non-interactive",
    "eas-build:ios:feature": "eas build -p ios --profile feature-test --non-interactive",
    "eas-build:ios:gsd": "eas build -p ios --profile gsd --non-interactive",
    "eas-build:ios:preview": "eas build -p ios --profile preview --non-interactive",
    "eas-build:ios:production": "eas build -p ios --profile production",
    "eas-submit:all:production": "eas submit -p all --profile production",
    "eas-submit:android:production": "eas submit -p android --profile production",
    "eas-submit:ios:production": "eas submit -p ios --profile production",
    "eject": "expo eject",
    "expo-publish:development": "expo publish --release-channel development",
    "expo-publish:gsd": "expo publish --release-channel gsd",
    "expo-publish:feature": "expo publish --release-channel feature-cu-2unfxxp",
    "expo-publish:preview": "expo publish --release-channel preview-8",
    "expo-publish:production": "expo publish --release-channel production-8",
    "format": "yarn lint --fix; yarn prettier --write",
    "generate-keystore-props": "dotenv -e .env -- sh ./scripts/generate-keystore-props.sh",
    "ios": "react-native run-ios",
    "lint": "eslint src e2e",
    "nuke:android": "cd android; ./gradlew clean; rm -rf .gradle build app/build $HOME/.gradle/caches/; cd ..",
    "nuke:ios": "cd ios; xcodebuild clean; pod cache clean --all; rm -rf build Pods; cd ..",
    "nuke": "yarn nuke:android; yarn nuke:ios; rm -rf node_modules; watchman watch-del-all; yarn cache clean --force; yarn; yarn start --reset-cache",
    "openurl:android": "adb shell am start -a \"android.intent.action.VIEW\" -d ",
    "openurl:ios": "xcrun simctl openurl booted",
    "pod-install": "cd ios; pod install --repo-update; cd ..",
    "postinstall": "cp -f src/assets/star.png node_modules/react-native-ratings/dist/images/; patch-package; yarn pod-install; yarn jetify; ",
    "postversion": "react-native-version --skip-tag --never-amend --never-increment-build",
    "prepare": "husky install",
    "prettier": "prettier --list-different src e2e",
    "start": "react-native start",
    "upload-source-maps:android": "sh ./scripts/upload-source-maps-android.sh",
    "upload-source-maps:ios": "sh ./scripts/upload-source-maps-ios.sh",
    "version": "npm version --no-git-tag-version",
    "web": "expo start --web"
  },
